{
  "name": "music-playlist-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "generate-types": "graphql-codegen",
    "lint:check": "eslint src --ext .ts,.js,.tsx,.jsx",
