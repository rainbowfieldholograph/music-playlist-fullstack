    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.2.3",
    "readme-md-generator": "^1.0.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^5.0.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^3.5.1"
